
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>

    <title>Index</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!--===============================================================================================-->
    <link rel="icon" type="image/png" href="~/loginweb/images/icons/favicon.ico" />
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/loginweb/vendor/bootstrap/css/bootstrap.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/loginweb/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/loginweb/fonts/iconic/css/material-design-iconic-font.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/loginweb/vendor/animate/animate.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/loginweb/vendor/css-hamburgers/hamburgers.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/loginweb/vendor/animsition/css/animsition.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/loginweb/vendor/select2/select2.min.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/loginweb/vendor/daterangepicker/daterangepicker.css">
    <!--===============================================================================================-->
    <link rel="stylesheet" type="text/css" href="~/loginweb/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/loginweb/css/main.css">
    <!--===============================================================================================-->
</head>
<body>
    <form action="/UyeGiris/Index" method="post">
        <div class="limiter">

            <div class="container-login100">
                <div class="wrap-login100 p-t-85 p-b-20">
                    <form class="login100-form validate-form">
                        <span class="login100-form-title p-b-70">
                            Uye Giris
                        </span>
                        <span class="login100-form-avatar">
                            <img src="~/loginweb/images/avatar-01.jpg" alt="AVATAR">
                        </span>

                        <div class="wrap-input100 validate-input m-t-85 m-b-35" data-validate="Enter username">
                            <input class="input100" type="text" name="username">
                            <span class="focus-input100" data-placeholder="Kullanıcı Adı"></span>
                        </div>

                        <div class="wrap-input100 validate-input m-b-50" data-validate="Enter password">
                            <input class="input100" type="password" name="pass">
                            <span class="focus-input100" data-placeholder="Sifre"></span>
                        </div>
                    </form>

                    <div class="container-login100-form-btn">
                        <button type="submit" class="login100-form-btn">
                            Login
                        </button>
                    </div>

                    <ul class="login-more p-t-190">
                        <li class="m-b-8">
                            <span class="txt1">
                                Sifremi Unuttum
                            </span>

                            <a href="#" class="txt2">
                                Kullanıcı Adı / Sifre ?
                            </a>
                        </li>

                        <li>
                            <span class="txt1">
                                Hesabınız yok mu?
                            </span>

                            <a href="#" class="txt2">
                                Uye OL
                            </a>
                        </li>
                    </ul>
    </form>
    </div>
    </div>
    </div>


    <div id="dropDownSelect1"></div>

    <!--===============================================================================================-->
    <script src="~/loginweb/vendor/jquery/jquery-3.2.1.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/loginweb/vendor/animsition/js/animsition.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/loginweb/vendor/bootstrap/js/popper.js"></script>
    <script src="~/loginweb/vendor/bootstrap/js/bootstrap.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/loginweb/vendor/select2/select2.min.js"></script>
    <!--===============================================================================================-->
    <script src="~/loginweb/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/loginweb/vendor/daterangepicker/daterangepicker.js"></script>
    <!--===============================================================================================-->
    <script src="~/loginweb/vendor/countdowntime/countdowntime.js"></script>
    <!--===============================================================================================-->
    <script src="~/loginweb/js/main.js"></script>

</body>
</html>
